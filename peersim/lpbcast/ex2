# this one adds more dynamism. Nodes come and go according to the
# configured schedule.

random.seed 1234569890

simulation.cycles 100

control.sh Shuffle

overlay.size 1
overlay.maxSize 10000

protocol.0 lpbcast.SimpleLpbcast
#protocol.0.l 10
protocol.0.F 3
protocol.0.subs 20
protocol.0.unSubs 20
protocol.0.unSubsTout 50

control.0 DynamicNetwork
control.0.add 1000
control.0.step 1
control.0.until 5
control.0.init.0 RandNI
control.0.init.0.protocol 0
control.0.init.0.k 2

control.1 lpbcast.Unsubscribe
control.1.add -10
control.1.step 1
control.1.from 8
control.1.protocol 0

control.o0 peersim.reports.DegreeStats
control.o0.protocol 0

control.o1 peersim.reports.ConnectivityObserver
control.o1.protocol 0

